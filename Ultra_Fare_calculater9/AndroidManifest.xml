<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.ultraneo.farecalculator9"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:icon="@drawable/fareicon"
        android:label="@string/app_name" >
         <uses-library android:name="com.google.android.maps" />
        <activity
            android:label="@string/app_name"
            android:name=".Fareccalculator9Activity" >      
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UltralocatorActivity"
            android:theme = "@style/LightTheme" 
         android:label="Point on map"
         
         ></activity>
        <activity android:name=".searchlisters"
            android:theme = "@style/LightTheme" 
         android:label="Search"
         
         ></activity>
        <activity android:name=".setting"
             
         android:label="Settings"
         
         ></activity>
            
        <activity android:name=".MapRouteActivity"
            android:theme = "@style/LightTheme" 
         android:screenOrientation="portrait"   
         android:label="Get Fare"
         
         ></activity>
            
         <activity android:name=".SwipeTest"  android:screenOrientation="portrait" android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" ></activity>  
    </application>

</manifest>